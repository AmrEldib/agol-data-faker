{
  "id": "itemSummary",
  "type": "object",
  "description": "User Item",
  "title": "item",
  "properties": {
    "id": {
      "id": "id",
      "type": "string",
      "description": "The unique ID for this item"
    },
    "owner": {
      "id": "owner",
      "type": "string",
      "description": "The username of the user who owns this item"
    },
    "created": {
      "id": "created",
      "type": "integer",
      "description": "The date the folder was created. Shown in UNIX time"
    },
    "modified": {
      "id": "modified",
      "type": "integer",
      "description": "The date the folder was last modified. Shown in UNIX time"
    },
    "guid": {
      "id": "guid",
      "type": [ "string", "null" ],
      "description": "Unique global ID"
    },
    "name": {
      "id": "name",
      "type": [ "string", "null" ],
      "description": "The file name of the item for file types. Read-only"
    },
    "title": {
      "id": "title",
      "type": "string",
      "description": "The title of the item. This is the name that's displayed to users and by which they refer to the item. Every item must have a title"
    },
    "type": {
      "id": "type",
      "type": "string",
      "description": "The GIS content type of this item. Example types include Web Map, Map Service, Shapefile, and Web Mapping Application. See the overview section of Items and item types to get an understanding of the item type hierarchy",
      "enum": [
        "Web Map",
        "CityEngine Web Scene",
        "Web Scene",
        "Pro Map",
        "Feature Service",
        "Map Service",
        "Image Service",
        "KML",
        "WMS",
        "Feature Collection",
        "Feature Collection Template",
        "Geodata Service",
        "Globe Service",
        "Geometry Service",
        "Geocoding Service",
        "Network Analysis Service",
        "Geoprocessing Service",
        "Workflow Manager Service",
        "Web Mapping Application",
        "Mobile Application",
        "Code Attachment",
        "Operations Dashboard Add In",
        "Operation View",
        "Operations Dashboard Extension",
        "Native Application",
        "Native Application Template",
        "Native Application Installer",
        "Symbol Set",
        "Color Set",
        "Shapefile",
        "File geodatabase",
        "CSV",
        "CAD Drawing",
        "Service Definition",
        "Document Link",
        "Microsoft Word",
        "Microsoft Powerpoint",
        "Microsoft Excel",
        "PDF",
        "Image",
        "Visio Document",
        "iWork Keynote",
        "iWork Pages",
        "iWork Numbers",
        "Map Document",
        "Map Package",
        "Mobile Basemap Package",
        "Tile Package",
        "Project Package",
        "Task File",
        "ArcPad Package",
        "Explorer Map",
        "Globe Document",
        "Scene Document",
        "Published Map",
        "Map Template",
        "Windows Mobile Package",
        "Layout",
        "Project Template",
        "Layer",
        "Layer Package",
        "Explorer Layer",
        "Desktop Style",
        "Geoprocessing Package",
        "Geoprocessing Package (Pro Version)",
        "Geoprocessing Sample",
        "Locator Package",
        "Rule Package",
        "Raster function template",
        "Workflow Manager Package",
        "Desktop Application",
        "Desktop Application Template",
        "Code Sample",
        "Desktop Add In",
        "Explorer Add In"
      ]
    },
    "typeKeywords": {
      "id": "typeKeywords",
      "type": "array",
      "description": "A set of keywords that further describes the type of this item. Each item is tagged with a set of type keywords that are derived based on its primary type",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "description": {
      "id": "description",
      "type": [ "string", "null" ],
      "description": "Item's description"
    },
    "tags": {
      "id": "tags",
      "type": "array",
      "description": "User defined tags that describe the item",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    },
    "snippet": {
      "id": "snippet",
      "type": [ "string", "null" ],
      "description": "A short summary description of the item"
    },
    "thumbnail": {
      "id": "thumbnail",
      "type": [ "string", "null" ],
      "description": "The URL to the thumbnail used for the item. Relative to Item's URL"
    },
    "extent": { "$ref": "extent" },
    "spatialReference": {
      "id": "spatialReference",
      "type": [ "string", "null" ],
      "description": "The coordinate system of the item"
    },
    "accessInformation": {
      "id": "accessInformation",
      "type": [ "string", "null" ],
      "description": "Information on the source of the item and its copyright status"
    },
    "licenseInfo": {
      "id": "licenseInfo",
      "type": [ "string", "null" ],
      "description": "Any license information or restrictions"
    },
    "culture": {
      "id": "culture",
      "type": "string",
      "description": "The item locale information (language and country)"
    },
    "url": {
      "id": "url",
      "type": [ "string", "null" ],
      "description": "The URL for the resource represented by the item. Applies only to items that represent web-accessible resources such as map services",
      "format": "uri"
    },
    "access": {
      "id": "access",
      "type": "string",
      "description": "Indicates the level of access to this item: private, shared, org, or public",
      "enum": [ "private", "shared", "org", "public" ]
    },
    "size": {
      "id": "size",
      "type": "integer",
      "description": "File size"
    },
    "numComments": {
      "id": "numComments",
      "type": "integer",
      "description": "Number of comments on this item"
    },
    "numRatings": {
      "id": "numRatings",
      "type": "integer",
      "description": "Number of ratings for this item"
    },
    "avgRating": {
      "id": "avgRating",
      "type": "integer",
      "description": "Average rating"
    },
    "numViews": {
      "id": "numViews",
      "type": "integer",
      "description": "Total number of views for this item"
    }
  },
  "required": [
    "id",
    "owner",
    "created",
    "modified",
    "guid",
    "name",
    "title",
    "type",
    "typeKeywords",
    "description",
    "tags",
    "snippet",
    "thumbnail",
    "extent",
    "spatialReference",
    "accessInformation",
    "licenseInfo",
    "culture",
    "url",
    "access",
    "size",
    "numComments",
    "numRatings",
    "numViews",
    "avgRating"
  ]
}
